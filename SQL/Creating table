--Creates Table to house data summarized by 538

CREATE TABLE nflelo (		
	date_played TEXT NOT NULL,	
	season SMALLINT CHECK(season>0),	
	neutral SMALLINT,	
	playoff VARCHAR(1),	
	team_home VARCHAR(3) NOT NULL,	
	team_away VARCHAR(3) NOT NULL,	
	elohome_pre NUMERIC,	
	eloaway_pre NUMERIC,	
	elo_prob_home DOUBLE PRECISION,	
	elo_prob_away DOUBLE PRECISION,	
	elo_home_post NUMERIC,	
	elo_away_post NUMERIC,	
	qbelo_home_pre NUMERIC,	
	qbelo_away_pre NUMERIC,	
	qb_home VARCHAR(40),	
	qb_away VARCHAR(40),	
	qb_home_value_pre NUMERIC,	
	qb_away_value_pre NUMERIC,	
	qb_home_adj NUMERIC,	
	qb_away_adj NUMERIC,	
	qbelo_prob_home DOUBLE PRECISION,	
	qbelo_prob_away DOUBLE PRECISION,	
	qb_home_game_value NUMERIC,	
	qb_away_game_value NUMERIC,	
	qb_home_value_post NUMERIC,	
	qb_away_value_post NUMERIC,	
	qbelo_home_post NUMERIC,	
	qbelo_away_post NUMERIC,	
	score_home SMALLINT,	
	score_away SMALLINT	
);		
